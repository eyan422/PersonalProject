package phone;

class Phone {
	private boolean tested;
	public void setTested(boolean val) {
		tested = val;
	}
	public boolean isTested() {
		return tested;
	}
	
	static public void main(String args[]){
		Phone p = new Phone();
		p.setTested(true);
		System.out.println(p.isTested());		
	}
	
